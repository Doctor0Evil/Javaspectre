{
  "profiles": [
    {
      "name": "Mistral-AI Spectral Profile",
      "id": "mistral-spectral",
      "theme": "Mistral-AI Spectral",
      "version": "1.0.0",
      "engine": {
        "provider": "mistral",
        "model": "mistral-large-latest",
        "response_format": "json_object",
        "temperature": 0.3,
        "top_p": 0.95,
        "max_tokens": 4096
      },
      "resonance": {
        "threshold": 70,
        "warn_below": 50,
        "critical_below": 40,
        "max_depth": 32,
        "max_nodes": 10000
      },
      "aln": {
        "enabled": true,
        "compatibility": true,
        "default_output_format": "json",
        "mode": "spectral-introspection",
        "attach_session_metadata": true
      },
      "scanner": {
        "include_prototype": true,
        "include_non_enumerable": false,
        "strict_cycle_handling": true
      },
      "logging": {
        "level": "info",
        "pretty_print": true,
        "include_input_preview": true
      },
      "integration": {
        "cli_enabled": true,
        "http_api_enabled": false,
        "default_cli_command": "scan-resonance"
      }
    },
